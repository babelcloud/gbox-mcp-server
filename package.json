{
  "name": "@gbox.ai/mcp-server",
  "version": "0.1.13",
  "description": "MCP server exposing Gbox Android control tools via Model Context Protocol",
  "homepage": "https://gbox.ai",
  "author": "Gbox Team",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "@gbox.ai/mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "inspect": "npx @modelcontextprotocol/inspector tsx src/index.ts",
    "dev": "node --loader ts-node/esm --no-warnings src/index.ts",
    "md2ts": "bin/md2ts.sh",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "prepack": "pnpm build"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "gbox",
    "ai",
    "android",
    "control"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babelcloud/gbox-mcp-server.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "axios": "^1.11.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "gbox-sdk": "^0.37.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/express": "^5.0.1",
    "@types/node": "^20.4.2",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.2.0",
    "typescript-eslint": "^8.40.0"
  },
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
